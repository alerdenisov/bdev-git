{{#with data.channel}}
<div class="live__camera live__camera_main">
  {{#if isTwitch}}
  <object type="application/x-shockwave-flash"
            data="http://www.twitch.tv/widgets/live_embed_player.swf?channel={{channelId}}"
            bgcolor="#000000">
    <param  name="allowFullScreen"
            value="true" />
    <param  name="allowScriptAccess"
            value="always" />
    <param  name="allowNetworking"
            value="all" />
    <param  name="movie"
            value="http://www.twitch.tv/widgets/live_embed_player.swf" />
    <param  name="flashvars"
            value="hostname=www.twitch.tv&channel={{channelId}}&auto_play=true&start_volume=50" />
  </object>
  {{else}}
  <object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" width="100%" height="100%" id="cyberplayer_jw">
      <param name="movie" value="http://cybergame.tv/players/cyberplayer42.swf?channel={{channelId}}&autostart=true">
      <param name="quality" value="high">
      <param name="bgcolor" value="#000000">
      <param name="wmode" value="opaque">
      <param name="allowScriptAccess" value="always">
      <param name="allowFullScreen" value="true">
      <embed
        src="http://cybergame.tv/players/cyberplayer42.swf?channel={{channelId}}&autostart=true"
        auto = "true" quality="high" bgcolor="#000000" width="100%" height="100%"
        name="cyberplayer_jw" wmode="opaque" allowfullscreen="true"
        allowscriptaccess="always" type="application/x-shockwave-flash"
        pluginpage="http://www.macromedia.com/go/getflashplayer">
  </object>
  {{/if}}
</div>
{{/with}}
